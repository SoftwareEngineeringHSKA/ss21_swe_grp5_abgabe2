{
  "name": "acme",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run prettier",
    "start": "ng serve --port=4200 --open=true --ssl --ssl-key=config/key.pem --ssl-cert=config/certificate.cer",
    "start:http": "ng serve --port=4200 --configuration production",
    "start:de": "ng serve --configuration=de --port=4200 --open=true --ssl --ssl-key=config/key.pem --ssl-cert=config/certificate.cer",
    "start:sirv": "sirv dist/acme --dev --gzip --http2 --cert config/certificate.cer --key config/key.pem --single",
    "start:es": "es-dev-server --port=4200 --root-dir=dist/acme --http2 --open --watch",
    "start:hs": "http-server --port 4200 --ssl --key config\\key.pem --cert config\\certificate.cer --proxy https://localhost:3000 -c-1 .... dist\\acme",
    "start:ORIG": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration=development --watch",
    "browserslist": "npx browserslist",
    "clean": "ts-node -P scripts/tsconfig.json scripts/clean",
    "docker-build": "ts-node scripts/docker-build",
    "doc": "typedoc --verbose --options typedoc.json src",
    "eslint": "npx eslint src/**/*.ts scripts/*.ts",
    "i18n": "ng xi18n --output-path=src/locale --out-file=messages.de.xlf",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "lighthouse:hs": "npx lighthouse https://www.h-ka.de/home/ --view --output-path=temp/www.h-ka.de.html",
    "lighthouse:hskampus": "npx lighthouse http://www.iwi.h-ka.de/hskampus-web/ --view --output-path=temp/app.hskampus.de.html --chrome-flags='--disable-web-security'",
    "local-web-server": "npx ws",
    "port-forward": "ts-node scripts/port-forward",
    "prettier": "npx prettier --write src/**/*.ts src/**/*.html src/**/*.scss scripts/*.ts",
    "sonarqube": "ts-node --script-mode scripts/sonarqube",
    "presource-map-explorer": "npm run build -- --source-map",
    "source-map-explorer": "npx source-map-explorer dist/acme/*-es2015*.js* --html source-map-explorer.html",
    "stylelint": "npx stylelint src/app/**/*.scss",
    "webhint": "hint https://localhost:4200",
    "prewebpack-bundle-analyzer": "npm run build -- --stats-json",
    "webpack-bundle-analyzer": "npx webpack-bundle-analyzer dist/acme/stats.json dist/acme",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.2",
    "@angular/cdk": "12.0.2",
    "@angular/common": "12.0.2",
    "@angular/compiler": "12.0.2",
    "@angular/core": "12.0.2",
    "@angular/forms": "12.0.2",
    "@angular/localize": "12.0.2",
    "@angular/material": "12.0.2",
    "@angular/platform-browser": "12.0.2",
    "@angular/platform-browser-dynamic": "12.0.2",
    "@angular/pwa": "12.0.2",
    "@angular/router": "12.0.2",
    "@angular/service-worker": "12.0.2",
    "@swimlane/ngx-charts": "18.0.1",
    "loglevel": "1.7.1",
    "rxjs": "6.6.7",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.2",
    "@angular-eslint/builder": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/schematics": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "12.0.2",
    "@angular/compiler-cli": "12.0.2",
    "@angular/language-service": "12.0.2",
    "@types/inquirer": "7.3.1",
    "@types/minimist": "1.2.1",
    "@types/node": "15.6.1",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "es-dev-server": "2.1.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-rxjs": "3.3.2",
    "eslint-plugin-unicorn": "32.0.1",
    "hint": "6.1.3",
    "inquirer": "8.1.0",
    "jest": "27.0.1",
    "jest-preset-angular": "9.0.0",
    "leasot": "12.0.0",
    "lighthouse": "7.5.0",
    "local-web-server": "4.2.1",
    "lws-redirect": "2.1.2",
    "minimist": "1.2.5",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "slash": "4.0.0",
    "source-map-explorer": "2.5.2",
    "stylelint": "13.13.1",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-config-standard": "22.0.0",
    "ts-jest": "27.0.1",
    "typescript": "4.2.4",
    "webpack-bundle-analyzer": "4.4.2"
  }
}
